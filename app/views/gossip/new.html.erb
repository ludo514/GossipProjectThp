<!DOCTYPE html>
<html>
	<head>
		<title></title>
	</head>
	<body>
	  <%= form_tag url_for(action:'create'), method: "post" do %>
	  			<div class="form-group">
	  				<%= label_tag 'Title'%><br>
	      			<%= text_field_tag 'title'%>
				</div>
				<div class="form-group"> 
					<%= label_tag 'Content' %><br>
	      			<%= text_area_tag 'content'%>
				</div>
	      <%= submit_tag "Create Post" %>
    	<% end %>
	</body>
</html>