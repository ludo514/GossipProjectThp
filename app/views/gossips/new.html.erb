<!DOCTYPE html>
<html>
	<head>
		<title></title>
	</head>
	<body>
		<%= form_for @user do |f|%>
			<div class="form-group">
				<label>Titre de l'article</label>
				<%= f.text_field :title, class:'form-control'%>
			</div>
			<div class="form-group">
				<label>Contenue de l'artcle</label>
				<%= f.text_area :content, class:'form-control'%>
			</div>
			<div class="form-group">
				<%= f.submit "Créé"%>
			</div>
		<%end%>
	</body>
</html>
